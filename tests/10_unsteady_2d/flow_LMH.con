{
  problem = {
    TYPE = "SequentialCoupling", 
    description = "Test10 - Unsteady flow in 2D, Lumped Mixed Hybrid", 
    mesh = {
      mesh_file = "${INPUT}/test10.msh"
    }, 

    primary_equation = {
      TYPE = "Unsteady_LMH", 

      bulk_data = [
        {
            region = "plane",
            cond_anisothropy = 0.02,
            init_pressure = 0,          // default
            storativity = 1             // default
        }
      ],

      bc_data = [
        { region = ".bc_inflow",
          bc_type = "dirichlet",
          bc_pressure = 100  
        },
        { region = ".bc_outflow",
          bc_type = "dirichlet",
          bc_pressure = 0  
        }
      ],

      n_schurs = 2, 
      output = {
        output_stream = {
          REF = "/system/output_streams"
        }, 
        pressure_p0 = "flow_output_stream", 
        pressure_p1 = "flow_output_stream", 
	velocity_p0 = "flow_output_stream",
        save_step = {
          REF = "/problem/save_step"
        }
      }, 
      solver = {
        TYPE = "Petsc", 
        accuracy = 1e-07, 
        parameters = ""
      }, 
      time = {
        REF = "../../time"
      }
    }, 
  
    save_step = 0.1, 
    time = {
      end_time = 0.5, 
      init_dt = 0.01
    }
  },
 
  system = {
    output_streams = {
        file = "./test10.pvd", 
        format = {
          TYPE = "vtk", 
          variant = "ascii"
        }, 
        name = "flow_output_stream"
    }
  }
}