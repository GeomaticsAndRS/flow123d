{
  problem = {
    TYPE = "SequentialCoupling", 
    description = "Test05 - Steady flow with Dirichlet/Neumann/Newton boundary condition", 
    mesh = {
      mesh_file = "${INPUT}/square_xz.msh"
    }, 

    primary_equation = {
      TYPE = "Steady_MH", 
      n_schurs = 2, 

      input_fields= [
        { region = "plane",
          anisotropy = 1
        },
        { region = ".bottom",
          bc_type = "neumann" // zero flux
        },
        { region = ".right",
          bc_type = "dirichlet",
          bc_piezo_head = 1.25
        },
        { region = ".top",
          bc_type = "seepage",
          bc_pressure = 0,
          bc_flux = 0
        },
        { region = ".left",
          bc_type = "dirichlet",
          bc_piezo_head = 0.25
        }
      ],

      output = {
        output_stream = {
          REF = "/system/output_stream"
        }, 
        output_fields = [ "piezo_head_p0", "pressure_p0", "pressure_p1", "velocity_p0" ]
      }, 

      balance = true,

      solver = {
        TYPE = "Petsc", 
        a_tol = 1e-07
      } 
    }
  }, 

  system = {
    output_stream = {
        file = "./test05.pvd", 
        format = {
          TYPE = "vtk", 
          variant = "ascii"
        }, 
        name = "flow_output_stream"
      }
  }
}