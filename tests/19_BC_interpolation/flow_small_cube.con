{
  problem = {
    TYPE = "SequentialCoupling", 
    description = "Interpolated boundary condition - small cube", 
    material = "input/small_cube.mtr", 
    mesh = {
      mesh_file = "input/small_mesh.msh",      
    }, 
    primary_equation = {
      TYPE = "Steady_MH", 
      bc_data=[
        {
          region = "IMPLICIT BOUNDARY",
          bc_type = "dirichlet",
          bc_pressure = {
            TYPE = "FieldInterpolatedP0",
            gmsh_file="input/large_cube.msh",
            field_name="piezo_head_elements_[L]"
          }
        }
      ],
      bulk_data=[
        {
          r_set = "BULK",
          anisotropy = 1
        }
      ],
      n_schurs = 2, 
      output = {
        balance_output = "water_balance.out", 
        output_stream = {
          REF = "/system/output_streams/0"
        }, 
        piezo_head_p0 = "flow_output_stream", 
        pressure_p0 = "flow_output_stream", 
        pressure_p1 = "flow_output_stream", 
        raw_flow_output = "./raw_output_small.txt", 
        velocity_p0 = "flow_output_stream"
      }, 
      solver = {
        TYPE = "Petsc", 
        accuracy = 1e-07
      },
    }, 
    save_step = 0.1, 
    secondary_equation = null
  }, 
  system = {
    output_streams = [
      {
        file = "./small_cube.pvd", 
        format = {
          TYPE = "vtk", 
          variant = "ascii"
        }, 
        name = "flow_output_stream"
      }
    ], 
    pause_after_run = false
  } 
}
