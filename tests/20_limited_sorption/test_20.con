{
  problem = {
    TYPE = "SequentialCoupling", 
    description = "TestXXX-pork",
    mesh = {
      mesh_file = "${INPUT}/test_20.msh"
    }, 
    
    primary_equation = {
      TYPE = "Steady_MH",
      
      bulk_data = [
        { region = "rectangle", 
          conductivity = 0.125
        }
      ],
      
      bc_data = [
        { r_set = "BOUNDARY",
          bc_type = "dirichlet",
          bc_pressure = { TYPE="FieldFormula", value = "x"}
        }
      ],
       
      output = {
        output_stream = {
          REF = "/output_streams/0"
        }, 
        pressure_p0 = "flow_output_stream", 
        pressure_p1 = "flow_output_stream", 
        velocity_p0 = "flow_output_stream"
      }, 

      solver = { 
        TYPE = "Petsc",
        a_tol = 1e-7 
      }
    }, 
    
    secondary_equation = {
      TYPE = "TransportOperatorSplitting",
   
      substances = ["A", "B", "C", "D", "G", "H"],
      
      bulk_data = [
        { r_set = "ALL",
          init_conc = 0,
          por_m = 0.25 
        }
      ],
      
      bc_data = [
        { r_set = "BOUNDARY",
          bc_conc = 1.0
        }
      ],
      
      output = {
        conc_mobile_p0 = "transport_output_stream", 
        output_stream = {
          REF = "/output_streams/1"
        }, 
        save_step = 0.5
      }, 
      time = {
        end_time = 10.0
      }
    } 
  }, 
  
    output_streams = [
      {
        file = "test_20.pvd", 
        format = {
          TYPE = "vtk", 
          variant = "ascii"
        }, 
        name = "flow_output_stream"
      }, 
      {
        file = "transport_test_20.pvd", 
        format = {
          TYPE = "vtk", 
          variant = "ascii"
        }, 
        name = "transport_output_stream"
      }
    ]
 

}